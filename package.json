{
  "name": "highline",
  "version": "0.0.0",
  "engines": {
    "node": "0.10.x"
  },
  "description": "Transforms GitHub links in a Markdown document into code blocks.",
  "keywords": [
    "markdown",
    "github",
    "code"
  ],
  "main": "./lib/",
  "scripts": {
    "style": "./node_modules/.bin/crispy ./lib/ ./test/",
    "test": "npm run style && ./node_modules/.bin/mocha --reporter spec -r should --grep ''"
  },
  "dependencies": {
    "bluebird": "^2.9.13",
    "handlebars": "^3.0.0",
    "joi": "^6.0.4",
    "lodash": "^3.5.0",
    "wreck": "^5.2.0"
  },
  "devDependencies": {
    "crispy": "^3.1.2",
    "mocha": "^2.2.1",
    "should": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/highline"
  }
}
